
{% trans_default_domain 'FOSUserBundle' %}

<h1>Modifier mon profil</h1>

{{ form_start(form, {'method': 'post', 'action': path('fos_user_profile_edit'), 'attr': { 'class': 'fos_user_profile_edit' } }) }}
    {{ form_row(form.username , { 'label': 'Prénom' }) }}
    {{ form_row(form.last_name, { 'label': 'Nom' }) }}
    {{ form_row(form.email, { 'label': 'Adresse email' }) }}
    {{ form_row(form.phone_number, { 'label': 'Numéro de téléphone' }) }}
    <input type="submit"></input>
{{ form_end(form) }}